{
  "name": "npm-registry",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "lint": "eslint ./{packages,docs} --ext js,jsx",
    "format": "prettier --write --ignore-unknown .",
    "install:docs": "npm --prefix=docs install",
    "install:packages": "lerna bootstrap --hoist --ignore-prepublish -- --no-package-lock",
    "postinstall": "run-s install:*",
    "develop:docs": "npm --prefix=docs run storybook",
    "develop:packages": "lerna run develop --stream",
    "develop": "run-s build:packages && run-p develop:*",
    "build:docs": "npm --prefix=docs run build-storybook",
    "build:packages": "lerna run build",
    "build": "run-s build:packages build:docs",
    "cleanup": "git clean -fdx"
  },
  "dependencies": {
    "react": ">=16.40.0",
    "react-dom": ">=16.40.0",
    "three": ">=0.128.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.2",
    "@commitlint/cli": "12.1.1",
    "babel-loader": "8.2.2",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.23.2",
    "husky": "6.0.0",
    "lerna": "^4.0.0",
    "lint-staged": "10.5.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1"
  }
}
